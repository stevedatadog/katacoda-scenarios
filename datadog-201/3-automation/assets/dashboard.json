{
    "title": "Stately! Dashboard",
    "description": "Created using NodeJS",
    "widgets": [
        {
            "layout": {
                "x": 1,
                "y": 0,
                "width": 32,
                "height": 37
            },
            "definition": {
                "title": "Redis System CPU Usage",
                "title_size": "16",
                "time": {
                    "live_span": "30m"
                },
                "type": "alert_graph",
                "alert_id": "35938540",
                "viz_type": "timeseries"
            }
        },
        {
            "layout": {
                "x": 34,
                "y": 0,
                "width": 32,
                "height": 18
            },
            "definition": {
                "title": "Redis Up",
                "title_size": "16",
                "time": {
                    "live_span": "30m"
                },
                "type": "check_status",
                "check": "datadog.agent.check_status",
                "grouping": "check",
                "group": "check:redisdb"
            }
        },
        {
            "layout": {
                "x": 34,
                "y": 19,
                "width": 32,
                "height": 18
            },
            "definition": {
                "type": "note",
                "content": "This dashboard was created usin NodeJS in a **Datadog 201** lab.",
                "background_color": "purple",
                "font_size": "36",
                "text_align": "left",
                "vertical_align": "top",
                "show_tick": false,
                "tick_pos": "50%",
                "tick_edge": "left",
                "has_padding": true
            }
        },
        {
            "layout": {
                "x": 67,
                "y": 0,
                "width": 72,
                "height": 37
            },
            "definition": {
                "title": "Stately Error Logs",
                "title_size": "16",
                "type": "log_stream",
                "logset": "",
                "indexes": [],
                "query": "status:error",
                "sort": {
                    "column": "time",
                    "order": "desc"
                },
                "columns": [
                    "core_host",
                    "core_service",
                    "tag_source"
                ],
                "show_date_column": true,
                "show_message_column": true,
                "message_display": "expanded-md"
            }
        },
        {
            "layout": {
                "x": 1,
                "y": 38,
                "width": 103,
                "height": 50
            },
            "definition": {
                "title": "stately-app #env:dd201",
                "title_size": "13",
                "title_align": "center",
                "time": {
                    "live_span": "1h"
                },
                "type": "trace_service",
                "env": "dd201",
                "service": "stately-app",
                "span_name": "falcon.request",
                "show_hits": true,
                "show_errors": true,
                "show_latency": true,
                "show_breakdown": true,
                "show_distribution": true,
                "show_resource_list": false,
                "size_format": "large",
                "display_format": "three_column"
            }
        },
        {
            "layout": {
                "x": 105,
                "y": 38,
                "width": 34,
                "height": 50
            },
            "definition": {
                "title": "CPU user by image",
                "title_size": "16",
                "title_align": "left",
                "show_legend": false,
                "legend_layout": "auto",
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "time": {
                    "live_span": "1h"
                },
                "type": "timeseries",
                "requests": [
                    {
                        "q": "avg:docker.cpu.system{$scope} by {docker_image}.fill(0)",
                        "style": {
                            "palette": "cool"
                        },
                        "display_type": "line"
                    }
                ],
                "yaxis": {
                    "scale": "linear",
                    "label": "",
                    "include_zero": true,
                    "min": "auto",
                    "max": "auto"
                },
                "markers": []
            }
        }
    ],
    "template_variables": [],
    "layout_type": "free",
    "is_read_only": true,
    "notify_list": []
}