                "type": "group",
                "background_color": "blue",
                "show_title": true,
                "layout_type": "ordered",
                "widgets": [
                    {
                        "id": 3934063617127353,
                        "definition": {
                            "title": "Performance overview of most popular pages",
                            "title_size": "16",
                            "title_align": "left",
                            "type": "query_table",
                            "requests": [
                                {
                                    "conditional_formats": [
                                        {
                                            "metric": "pc75:@view.largest_contentful_paint",
                                            "comparator": ">",
                                            "palette": "red_on_white",
                                            "value": 4000000000
                                        },
                                        {
                                            "metric": "pc75:@view.largest_contentful_paint",
                                            "comparator": ">",
                                            "palette": "yellow_on_white",
                                            "value": 2500000000
                                        },
                                        {
                                            "metric": "pc75:@view.largest_contentful_paint",
                                            "comparator": "<=",
                                            "palette": "green_on_white",
                                            "value": 2500000000
                                        },
                                        {
                                            "metric": "pc75:@view.first_input_delay",
                                            "comparator": ">",
                                            "palette": "red_on_white",
                                            "value": 300000000
                                        },
                                        {
                                            "metric": "pc75:@view.first_input_delay",
                                            "comparator": ">",
                                            "palette": "yellow_on_white",
                                            "value": 100000000
                                        },
                                        {
                                            "metric": "pc75:@view.first_input_delay",
                                            "comparator": "<=",
                                            "palette": "green_on_white",
                                            "value": 100000000
                                        },
                                        {
                                            "metric": "pc75:@view.cumulative_layout_shift",
                                            "comparator": ">",
                                            "palette": "red_on_white",
                                            "value": 0.25
                                        },
                                        {
                                            "metric": "pc75:@view.cumulative_layout_shift",
                                            "comparator": ">",
                                            "palette": "yellow_on_white",
                                            "value": 0.1
                                        },
                                        {
                                            "metric": "pc75:@view.cumulative_layout_shift",
                                            "comparator": "<=",
                                            "palette": "green_on_white",
                                            "value": 0.1
                                        }
                                    ],
                                    "rum_query": {
                                        "index": "*",
                                        "search": {
                                            "query": "@type:view $env"
                                        },
                                        "group_by": [
                                            {
                                                "facet": "@view.url_path_group",
                                                "sort": {
                                                    "order": "desc",
                                                    "aggregation": "count"
                                                },
                                                "limit": 15
                                            }
                                        ],
                                        "multi_compute": [
                                            {
                                                "aggregation": "count"
                                            },
                                            {
                                                "facet": "@view.loading_time",
                                                "aggregation": "pc75"
                                            },
                                            {
                                                "facet": "@view.largest_contentful_paint",
                                                "aggregation": "pc75"
                                            },
                                            {
                                                "facet": "@view.first_input_delay",
                                                "aggregation": "pc75"
                                            },
                                            {
                                                "facet": "@view.cumulative_layout_shift",
                                                "aggregation": "pc75"
                                            }
                                        ]
                                    },
                                    "cell_display_mode": [
                                        "bar",
                                        "number",
                                        "number",
                                        "number",
                                        "number"
                                    ]
                                }
                            ],
                            "has_search_bar": "never",
                            "custom_links": [
                                {
                                    "link": "/dash/integration/rum_errors?tpl_var_pathGroup={{@view.url_path_group.value}}",
                                    "label": "Analyze errors for {{@view.url_path_group.value}}"
                                },
                                {
                                    "link": "/dash/integration/rum_resources?tpl_var_pathGroup={{@view.url_path_group.value}}",
                                    "label": "View resources for {{@view.url_path_group.value}}"
                                }
                            ]
                        },
                        "layout": {
                            "x": 0,
